{
  "name": "@3si-khanhnv/angular-github-actions",
  "version": "0.0.5",
  "description": "Application example built with Angular 15 and hosted on GitHub Pages using GitHub Actions.",
  "license": "MIT",
  "main": "./dist/glory-imo-dev/shared-components/index.d.ts",
  "types": "./dist/glory-imo-dev/shared-components/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:clean && ng build --project=@glory-imo-dev/shared-components",
    "build:clean": "rimraf ./dist",
    "build:tsc": "tsc -p tsconfig.json",
    "build:copy": "cpx ./projects/glory-imo-dev/shared-components/src/public-api.ts ./dist",
    "clear-autogen-files": "jest --clear-cache && rimraf ./dist/glory-imo-dev/shared-components",
    "release": "npm run build",
    "publish": "npm publish ./dist/glory-imo-dev/shared-components"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/3si-khanhnv/angular-github-actions.git"
  },
  "homepage": "https://github.com/3si-khanhnv/angular-github-actions#readme",
  "bugs": {
    "url": "https://github.com/3si-khanhnv/angular-github-actions/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/router": "^15.2.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.12.0",
    "release-please": "^16.5.0"
  },
  "devDependencies": {
    "@deboxsoft/cpx": "^1.5.0",
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.5",
    "@types/jasmine": "~4.3.1",
    "angular-cli-ghpages": "^1.0.7",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.5"
  }
}
